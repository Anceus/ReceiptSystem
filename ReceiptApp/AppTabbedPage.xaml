<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:ReceiptApp;assembly=ReceiptApp"
            x:Class="ReceiptApp.AppTabbedPage">

    <NavigationPage Title="Receipts">
        <x:Arguments>
            <local:ReceiptsPage Title="Receipts" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Dashboard">
        <x:Arguments>
            <local:DashboardPage Title="Dashboard" />
        </x:Arguments>
    </NavigationPage>
    

</TabbedPage>